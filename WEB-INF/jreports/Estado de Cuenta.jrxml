	<parameter name="Cliente" class="java.lang.String"/>
	<parameter name="Fecha_Inicial" class="java.lang.String"/>
	<parameter name="Fecha_Final" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT CT_RAZONSOCIAL,( SELECT sum(
((MC_CARGO-MC_ABONO) * if((select TC_PARIDAD from vta_tasacambio
where TC_MONEDA2 = if(MC_MONEDA = 0,1,MC_MONEDA) and tc_moneda1 = 2
 and TTC_ID = 4 AND TC_FECHA <= MC_FECHA ORDER BY TC_FECHA DESC limit 0,1) is not null,
(select TC_PARIDAD from vta_tasacambio
where TC_MONEDA2 = if(MC_MONEDA = 0,1,MC_MONEDA) and tc_moneda1 = 2
 and TTC_ID = 4 AND TC_FECHA <= MC_FECHA ORDER BY TC_FECHA DESC limit 0,1) ,
(select TC_PARIDAD from vta_tasacambio
where TC_MONEDA2 = if(MC_MONEDA = 0,1,MC_MONEDA) and tc_moneda1 = 2
 and TTC_ID = 4 AND TC_FECHA > MC_FECHA ORDER BY TC_FECHA asc limit 0,1)
)
)
) as Importe
FROM vta_mov_cte WHERE
CT_ID= $P{Cliente} AND MC_FECHA < $P{Fecha_Inicial} AND MC_ANULADO = 0) as SALDO_INICIAL
FROM vta_cliente WHERE
CT_ID= $P{Cliente};]]>
	</queryString>
