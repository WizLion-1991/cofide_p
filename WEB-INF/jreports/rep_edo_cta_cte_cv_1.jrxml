	</subDataset>
	<subDataset name="pagos_periodo" uuid="75b12d80-58cc-45c1-8d4c-0821a9700aca">
		<parameter name="Periodo" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="Moneda" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="Cliente" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT MC_ID, DATE_FORMAT(STR_TO_DATE(MC_FECHA,'%Y%m%d'),'%d/%m/%Y') AS FECHA,vta_cliente.CT_ID,MC_FOLIO, MC_ABONO,
MC_MONEDA,(select MON_DESCRIPCION from vta_monedas where MON_ID=MC_MONEDA  ) as NOM_MONEDA,vta_cliente.CT_DIASCREDITO
,vta_empresas.EMP_RAZONSOCIAL as EMPRESA
,(
select GROUP_CONCAT(DISTINCT vta_bcos.BC_DESCRIPCION) from  vta_mov_cta_bcos_rela,vta_mov_cta_bcos, vta_bcos where 
 vta_mov_cte.MC_ID = vta_mov_cta_bcos_rela.MC_ID and 
 vta_mov_cta_bcos_rela.MCB_ID = vta_mov_cta_bcos.MCB_ID and  
 vta_mov_cta_bcos.BC_ID = vta_bcos.BC_ID and
 vta_mov_cte.MC_ANULADO = 0 and vta_mov_cte.MC_ABONO> 0 ) as TBANCO,MC_ESPAGO,NC_ID,(select NC_FOLIO FROM vta_ncredito WHERE vta_ncredito.NC_ID = vta_mov_cte.NC_ID) as NC_FOLIO FROM vta_mov_cte,vta_cliente, vta_empresas
 where vta_mov_cte.CT_ID = vta_cliente.CT_ID AND MC_ANULADO = 0 AND MC_ABONO > 0
 AND vta_mov_cte.EMP_ID = vta_empresas.EMP_ID 
 AND vta_cliente.CT_ID = $P{Cliente} and vta_mov_cte.MC_MONEDA = $P{Moneda}  
 AND ((LEFT(vta_mov_cte.MC_FECHA,6) = $P{Periodo} )  ) order by DATEDIFF(CURDATE(),STR_TO_DATE(MC_FECHA,'%Y%m%d')) asc,MC_FOLIO]]>
		</queryString>
		<field name="MC_ID" class="java.lang.Long"/>
		<field name="FECHA" class="java.lang.String"/>
		<field name="CT_ID" class="java.lang.Long"/>
		<field name="MC_FOLIO" class="java.lang.String"/>
		<field name="MC_ABONO" class="java.math.BigDecimal"/>
		<field name="MC_MONEDA" class="java.lang.Long"/>
		<field name="NOM_MONEDA" class="java.lang.String"/>
		<field name="CT_DIASCREDITO" class="java.lang.Long"/>
		<field name="EMPRESA" class="java.lang.String"/>
		<field name="TBANCO" class="java.lang.String"/>
		<field name="MC_ESPAGO" class="java.lang.Integer"/>
		<field name="NC_ID" class="java.lang.Long"/>
		<field name="NC_FOLIO" class="java.lang.String"/>
	</subDataset>
	<parameter name="Cliente" class="java.lang.Integer"/>
	<parameter name="PathBase" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="Moneda" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<parameter name="Periodo" class="java.lang.String"/>
	<parameter name="ConSaldo" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT FAC_ID, DATE_FORMAT(STR_TO_DATE(FAC_FECHA,'%Y%m%d'),'%d/%m/%Y') AS FECHA,vta_cliente.CT_ID,FAC_FOLIO,FAC_RAZONSOCIAL,FAC_TOTAL,FAC_SALDO,FAC_IMPORTE,FAC_IMPUESTO1,FAC_RAZONSOCIAL, FAC_DIASCREDITO,
 DATEDIFF(CURDATE(),DATE_ADD(STR_TO_DATE(FAC_FECHA,'%Y%m%d'), INTERVAL FAC_DIASCREDITO DAY)) as dias   
,DATE_FORMAT(DATE_ADD(STR_TO_DATE(FAC_FECHA,'%Y%m%d'), INTERVAL FAC_DIASCREDITO DAY),'%d/%m/%Y') AS VENCIMIENTO
,FAC_MONEDA,(select MON_DESCRIPCION from vta_monedas where MON_ID=FAC_MONEDA  ) as NOM_MONEDA,vta_cliente.CT_DIASCREDITO
,1 as tasa
,if(FAC_DIASCREDITO = 0,if(DATEDIFF(CURDATE(), DATE_ADD(STR_TO_DATE(FAC_FECHA,'%Y%m%d'), INTERVAL FAC_DIASCREDITO DAY)) > 0,'VENCIDO','CONTADO'),if(DATEDIFF(CURDATE(), DATE_ADD(STR_TO_DATE(FAC_FECHA,'%Y%m%d'), INTERVAL FAC_DIASCREDITO DAY)) > 0, 'VENCIDO', 'VIGENTE')) AS ESTATUS 
,vta_empresas.EMP_RAZONSOCIAL as EMPRESA
 FROM vta_facturas,vta_cliente, vta_empresas
 where vta_facturas.CT_ID = vta_cliente.CT_ID AND FAC_ANULADA = 0 
 AND vta_facturas.EMP_ID = vta_empresas.EMP_ID  
 AND vta_cliente.CT_ID = $P{Cliente} 
 AND  if($P{ConSaldo} = 1,
 ((LEFT(vta_facturas.FAC_FECHA,6) <= $P{Periodo} AND vta_facturas.FAC_SALDO >= 1)  
 OR (select COUNT(vta_mov_cte.FAC_ID) from  vta_mov_cte where 
 vta_facturas.FAC_ID = vta_mov_cte.FAC_ID AND 
 LEFT(vta_mov_cte.MC_FECHA,6) = $P{Periodo} 
 AND LEFT(vta_facturas.FAC_FECHA,6) < $P{Periodo} 
 AND MC_ESPAGO = 1 AND MC_ANULADO = 0 ) )
 ,
 ((LEFT(vta_facturas.FAC_FECHA,6) < $P{Periodo} AND vta_facturas.FAC_SALDO >= 1 ) OR 
(LEFT(vta_facturas.FAC_FECHA,6) = $P{Periodo} ) 
 OR (select COUNT(vta_mov_cte.FAC_ID) from  vta_mov_cte where 
vta_facturas.FAC_ID = vta_mov_cte.FAC_ID AND 
LEFT(vta_mov_cte.MC_FECHA,6) = $P{Periodo} 
AND LEFT(vta_facturas.FAC_FECHA,6) < $P{Periodo} 
AND MC_ESPAGO = 1 AND MC_ANULADO = 0 ) )
 )
 order by DATEDIFF(CURDATE(),STR_TO_DATE(FAC_FECHA,'%Y%m%d')) asc,FAC_FOLIO]]>
	</queryString>
	<field name="FAC_ID" class="java.lang.Long"/>
	<field name="FECHA" class="java.lang.String"/>
	<field name="CT_ID" class="java.lang.Long"/>
	<field name="FAC_FOLIO" class="java.lang.String"/>
	<field name="FAC_RAZONSOCIAL" class="java.lang.String"/>
	<field name="FAC_TOTAL" class="java.math.BigDecimal"/>
	<field name="FAC_SALDO" class="java.math.BigDecimal"/>
	<field name="FAC_IMPORTE" class="java.math.BigDecimal"/>
	<field name="FAC_IMPUESTO1" class="java.math.BigDecimal"/>
	<field name="FAC_DIASCREDITO" class="java.lang.Integer"/>
	<field name="dias" class="java.lang.Long"/>
