	</parameter>
	<queryString>
		<![CDATA[SELECT *, DATE_FORMAT(STR_TO_DATE(MP_FECHA,'%Y%m%d'),'%d/%m/%Y') AS FECHA,
(PV_RAZONSOCIAL) AS PROVEEDOR,
(SELECT SC_NOMBRE FROM vta_sucursal WHERE vta_mov_prov.SC_ID=vta_sucursal.SC_ID) AS SUCURSAL,
(SELECT EMP_RAZONSOCIAL FROM vta_empresas WHERE vta_proveedor.EMP_ID=vta_empresas.EMP_ID) AS NOM_EMPRESA,
if(MP_ESPAGO > 0, if(MP_ANTICIPO = 1,'ANTICIPO','PAGO'),if(MP_USA_ANTICIPO = 1,'APLICAR ANTICIPO','FACTURA')) AS CONCEPTO,
(SELECT MON_DESCRIPCION FROM vta_monedas where vta_monedas.MON_ID =vta_mov_prov.MP_MONEDA)AS NOM_MONEDA,
(SELECT CXP_FOLIO FROM vta_cxpagar WHERE vta_cxpagar.CXP_ID=vta_mov_prov.CXP_ID) as TFOLIO,

/*obtenemos la paridad*/
if(if($P{Moneda} <> 0,$P{Moneda},vta_proveedor.MON_ID) = vta_mov_prov.MP_MONEDA , 1,
if(MP_TASAPESO <> 1 AND MP_TASAPESO <> 0 ,MP_TASAPESO,GetParidad(vta_mov_prov.MP_MONEDA,if($P{Moneda} <> 0,$P{Moneda},vta_proveedor.MON_ID),4,MP_FECHA) )
)
/*obtenemos la paridad*/
 as tasa
FROM vta_proveedor,vta_mov_prov WHERE vta_mov_prov.PV_ID=vta_proveedor.PV_ID and vta_proveedor.pv_id = $P{Proveedor}
AND MP_FECHA >= $P{Fecha_Inicial} AND MP_FECHA <= $P{Fecha_Final} AND MP_ANULADO = 0
 AND if($P{FacturaId} = 0, 1 = 1, vta_mov_prov.CXP_ID = $P{FacturaId} )
and if($P{SoloMonedaOrig} = 1,if(vta_mov_prov.CXP_ID = 0,vta_mov_prov.MP_MONEDA = $P{Moneda},(SELECT CXP_MONEDA FROM vta_cxpagar WHERE vta_cxpagar.CXP_ID=vta_mov_prov.CXP_ID) =$P{Moneda} ),1=1 )
ORDER BY MP_FECHA,MP_HORA;]]>
	</queryString>
